package com.slokam.test.automation.common.locators;

import org.openqa.selenium.By;

public interface Locators {

	
	public static By SEARCH_ELEMENT = By.id("q");
	public static By SIGNIN_BTN = By.xpath("//a[@title='Sign in']");
	public static By SIGNIN = By.cssSelector(".login");
	public static By REGISTRATION = By.cssSelector("a[href='/account/register']");
	public static By REGISTRATION_HEADING = By.xpath("//h2");
	public static By USERNAME = By.cssSelector("#user_login");
	public static By PASSWORD = By.cssSelector("#user_password");
	
	
	public static By ERROR_LOCATION = By.xpath("//span[@class='errorSpan']/input");
}
